<body>
		<%
		List<Empresa> lista = (List<Empresa>)request.getParameter("lista");

		%>

		
		out.println("<ul>");
		for (Empresa empresa : lista) {
			out.println("<li>" + empresa.getNome() + "</li>");
		}
		out.println("</ul>");
		out.println("</body></html>");

</body>
